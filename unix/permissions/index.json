                    "node": [
                        {
                            "@_TEXT": "$ chmod ug+rw sample",
                            "@_ID": "ID_864977651",
                            "@_CREATED": 1574868093306,
                            "@_MODIFIED": 1574868093306
                        },
                        {
                            "@_TEXT": "$ ls -ld sample",
                            "@_ID": "ID_1481316847",
                            "@_CREATED": 1574868093307,
                            "@_MODIFIED": 1574868093307
                        },
                        {
                            "@_TEXT": "drw-rw----   2 unixguy  unixguy       96 Dec  8 12:53 sample",
                            "@_ID": "ID_1403719885",
                            "@_CREATED": 1574868093324,
                            "@_MODIFIED": 1574868093324
                        },
                        {
                            "@_TEXT": "This command removes all permissions, allowing no one to read, write, or execute the file named sample.",
                            "@_ID": "ID_846571585",
                            "@_CREATED": 1574868093324,
                            "@_MODIFIED": 1574868093324
                        },
                        {
                            "@_TEXT": "$ chmod a-rwx sample",
                            "@_ID": "ID_1430656093",
                            "@_CREATED": 1574868093326,
                            "@_MODIFIED": 1574868093326
                        },
                        {
                            "@_TEXT": "$ ls -l sample",
                            "@_ID": "ID_1325930662",
                            "@_CREATED": 1574868093331,
                            "@_MODIFIED": 1574868093331
                        },
                        {
                            "@_TEXT": "----------   2 unixguy  unixguy       96 Dec  8 12:53 sample",
                            "@_ID": "ID_314314656",
                            "@_CREATED": 1574868093332,
                            "@_MODIFIED": 1574868093332
                        },
                        {
                            "@_TEXT": "The following command changes the permissions for the user and the group to read and execute only (no write permission) on sample.",
                            "@_ID": "ID_1114752646",
                            "@_CREATED": 1574868093332,
                            "@_MODIFIED": 1574868093332
                        },
                        {
                            "@_TEXT": "$ # Sample file permissions before command",
                            "@_ID": "ID_1268244799",
                            "@_CREATED": 1574868093333,
                            "@_MODIFIED": 1574868093333
                        },
                        {
                            "@_TEXT": "$ ls -ld sample",
                            "@_ID": "ID_1143048075",
                            "@_CREATED": 1574868093336,
                            "@_MODIFIED": 1574868093336
                        },
                        {
                            "@_TEXT": "drw-rw----   2 unixguy  unixguy       96 Dec  8 12:53 sample",
                            "@_ID": "ID_229967548",
                            "@_CREATED": 1574868093337,
                            "@_MODIFIED": 1574868093337
                        },
                        {
                            "@_TEXT": "$ chmod ug=rx sample",
